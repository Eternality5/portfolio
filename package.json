		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.12",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.6",
		"vitest": "^1.6.0"
	},
	"type": "module"
}
